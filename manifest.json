{
    "manifest_version": 3,
    "name": "CheatSheet",
    "description": "Enhance accessibility for Neurodivergence when browsing",
    "version": "1.0",
    "icons": {
        "16": "img/icon_16.png",
        "32": "img/icon_32.png",
        "48": "img/icon_48.png",
        "128": "img/icon_128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "img/icon_16.png",
            "32": "img/icon_32.png",
            "48": "img/icon_48.png",
            "128": "img/icon_128.png"
        },
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "scripting"]
}